{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "jsdoc-format": false,
        "interface-name": false,
        "no-empty-interface": false,
        "no-unused-expression": false,
        "object-literal-sort-keys": false,
        "semicolon": [
            true,
            "never"
        ],
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template"
        ]
    },
    "rulesDirectory": []
}